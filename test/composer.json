{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "anwire/cart": "dev-main",
        "anwire/checkout": "dev-main",
        "anwire/commonmark-unlink": "dev-main",
        "anwire/contracts": "dev-main",
        "anwire/dblog": "dev-main",
        "anwire/geo": "dev-main",
        "anwire/gql": "dev-main",
        "anwire/guest": "dev-main",
        "anwire/item": "dev-main",
        "anwire/mediable": "dev-main",
        "anwire/order": "dev-main",
        "anwire/payment": "dev-main",
        "anwire/shield": "dev-main",
        "anwire/support": "dev-main",
        "anwire/tax": "dev-main",
        "anwire/team": "dev-main",
        "barryvdh/laravel-dompdf": "^2.0",
        "commerceguys/addressing": "^2.0",
        "doctrine/dbal": "^3.6",
        "egulias/email-validator": "^4.0",
        "figment/laravel-mediable-filament-plugin": "dev-main",
        "filament/filament": "^3.0-stable",
        "guzzlehttp/guzzle": "^7.2",
        "hidehalo/nanoid-php": "^1.1",
        "laravel/framework": "^10.0",
        "laravel/jetstream": "^4.0",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "milon/barcode": "^10.0",
        "mll-lab/graphql-php-scalars": "^6.1",
        "mll-lab/laravel-graphiql": "^3.0",
        "nuwave/lighthouse": "^6.8",
        "plank/laravel-mediable": "^5.9",
        "timehunter/laravel-google-recaptcha-v3": "^2.5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "nunomaduro/larastan": "^2.6",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": [
        {
            "type": "path",
            "url": "./packages/**/*"
        }
    ],
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
